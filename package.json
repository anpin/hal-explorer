    "@angular/router": "~12.2.6",
    "bootstrap": "^5.1.1",
    "core-js": "^3.17.3",
    "http-status-codes": "^2.1.4",
    "rxjs": "~6.5.3",
    "tslib": "^2.3.1",
    "uri-templates": "^0.2.0",
    "webdriver-manager": "^12.1.8",
    "zone.js": "~0.11.4"
  },
