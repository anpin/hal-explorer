    "@angular-eslint/schematics": "16.0.2",
    "@angular-eslint/template-parser": "16.0.2",
    "@angular/cli": "^16.0.3",
    "@angular/compiler-cli": "^16.0.3",
    "@angular/language-service": "^16.0.3",
    "@cypress/schematic": "2.5.0",
    "@popperjs/core": "^2.11.8",
    "@types/jasmine": "~4.3.2",
    "@types/jasminewd2": "~2.0.10",
